zone "pve.datacenter.tsc" {
    type slave;
    file "db.pve.datacenter.tsc";
    masters { 192.168.2.55; };  # ns1 private IP
};

zone "168.192.in-addr.arpa" {
    type slave;
    file "db.192.168";
    masters { 192.168.2.56; };  # ns1 private IP
};
